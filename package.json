{
    "name": "filemeta-data",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "run-once-after-npm-install": "npx ts-patch install",
        "test": "jest --runInBand --detectOpenHandles --silent=false",
        "db:migrate": "rm -rf prisma/migrations && npx prisma migrate reset -f && npx prisma generate && npx prisma migrate dev --name init",
        "db:seed": "node --import tsx prisma/seeds/seed.ts"
    },
    "dependencies": {
        "@prisma/client": "^5.17.0",
        "prisma": "^5.16.1",
        "ts-runtime-checks": "^0.6.0",
        "uuid": "^10.0.0"
    },
    "devDependencies": {
        "@faker-js/faker": "^8.4.1",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.14.10",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^10.0.0",
        "jest": "^29.7.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.2",
        "ts-patch": "^3.2.1",
        "tsx": "^4.16.2",
        "typescript": "^5.5.3"
    }
}
